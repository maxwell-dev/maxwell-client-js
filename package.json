{
  "name": "maxwell-client",
  "version": "1.5.0",
  "description": "Maxwell client implementation for js/ts.",
  "author": "Chaoqian Xu <chaoranxu@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "serve": "npx webpack serve --config webpack.config.js --progress --open",
    "pack": "npx webpack --config webpack.config.js --progress",
    "connection": "npm run pack && node dist/connection.bundle.js",
    "requester": "npm run pack && node dist/requester.bundle.js",
    "subscriber": "npm run pack && node dist/subscriber.bundle.js",
    "test": "npx jest --coverage",
    "format": "npx prettier --write \"src/**/*.{js,ts}\" \"test/**/*.{js,ts}\" --ignore-path *.d.ts",
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern *.d.ts",
    "clean": "bin/clean.sh"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "repository": "https://github.com/maxwell-dev/maxwell-client-js.git",
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.5",
    "@xuchaoqian/abortable-promise": "^1.4.3",
    "@xuchaoqian/localstore": "^0.5.4",
    "cross-fetch": "^4.0.0",
    "maxwell-protocol": "^1.4.0",
    "maxwell-utils": "^0.12.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@msgpack/msgpack": "^2.8.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "babel-jest": "^29.7.0",
    "bufferutil": "^4.0.8",
    "canvas": "^2.11.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.3.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "utf-8-validate": "^6.0.3",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.15.1"
  }
}
